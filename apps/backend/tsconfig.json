{
  "compilerOptions": {
    "baseUrl": "src",
    "strict": false,
    "noImplicitAny": false,
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client"
    ],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "#root/*": [
        "./*"
      ]
    }
  }
}